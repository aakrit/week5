<h1>New Movie</h1>

<%= form_tag(movies_url) do %>
  <p>
    <%= label_tag :title %>
    <%= text_field_tag :title %>
  </p>

  <p>
    <%= label_tag :year %>
    <%= text_field_tag :year %>
  </p>

  <p><%= submit_tag %></p>
<% end %>


<%= form_for Movie.new do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :year %>
    <%= f.text_field :year %>
  </p>

  <p><%= f.submit %></p>
<% end %>
